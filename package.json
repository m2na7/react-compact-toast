{
  "name": "react-compact-toast",
  "version": "0.1.2",
  "private": false,
  "repository": "https://github.com/m2na7/react-compact-toast.git",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
