{
  "extends": "@cypress/code-coverage/nyc-config",
  "include": ["src/**/*.{js,jsx,ts,tsx}"],
  "exclude": [
    "src/**/*.cy.{js,jsx,ts,tsx}",
    "src/**/*.test.{js,jsx,ts,tsx}",
    "src/**/*.spec.{js,jsx,ts,tsx}",
    "src/index.ts",
    "src/app/**/*"
  ],
  "reporter": ["text", "json", "html", "lcov"],
  "report-dir": "coverage",
  "all": true,
  "check-coverage": true,
  "branches": 80,
  "functions": 80,
  "lines": 80,
  "statements": 80
}
